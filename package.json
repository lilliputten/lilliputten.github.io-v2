    "remark-comments": "^1.2.4",
    "remark-react": "^5.0.0",
    "typed-react-markdown": "^0.1.0"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/es6-shim": "^0.31.39",
    "@types/flux": "^3.1.8",
    "@types/jest": "^23.1.4",
    "@types/ramda": "^0.25.41",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-test-renderer": "^16.0.3",
    "dateformat": "^3.0.3",
