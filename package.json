    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "npm-package-user-scripts-list": "^0.1.10",
    "npm-run-all": "^4.1.3",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewire-typescript": "^2.0.2",
    "react-app-rewired": "1.5.2",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.6.3",
    "rebem-css": "^0.2.0",
    "serve": "^10.1.1",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "unusedDependencies": {
    "bem-react-core": "^2.0.0"
  },
  "jest": {
    "transform": {
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/node_modules/react-scripts/config/jest/fileTransform.js",
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "scripts": {
    "postinstall-patches": "cd \"!Patches\" && pwd && sh ./patch-tool-apply-all && cd ..",
    "postinstall-publish-dir": "( test ! -d build || git rm -f build ) && git submodule add -f git@github.com:lilliputten/lilliputten.github.io.git .publish",
    "postinstall-build-SAMPLE": "( test ! -d build || git rm -f build ) && git submodule add <repoPath> build",
    "postinstall": "echo --Run postinstall tasks-- && npm-run-all postinstall-patches postinstall-publish-dir",
    "start": "react-app-rewired start",
    "server": "echo --Start developer server-- && react-app-rewired start",
    "eslint": "echo --JS source code linting-- && eslint . --ext .js && echo No JS problems found",
    "tslint": "echo --TS source code linting-- && tslint -p . --exclude \"**/*.{js,jsx}\" && echo No TS problems found",
    "lint": "echo --Lint all source code-- && npm-run-all eslint tslint",
    "test": "echo --Run tests-- && jest",
    "tests": "echo --Run all lintings and tests-- && npm-run-all lint test",
    "make-build": "echo --Make production build-- && react-app-rewired build",
    "publish-build": "echo --Publish build to gitpages (via gulp-gh-pages, buggy)-- && gulp publish",
    "commit-build": "echo --Commit build to gitpages (via scripts)-- && cd .publish && rm -Rf && cp -Rfu ../build/* . && git add . -Av && git commit -am \"auto-commit\" && git push && cd ..",
    "serve-build": "echo --Run static server for build-- && serve -s build",
    "pack-build": "echo --Pack build for demo server-- && rm -f build-pack.zip && zip -rq build-pack.zip build",
    "eject-UNUSED": "react-scripts eject",
    "launcher": "npm-launcher",
    "gulp": "gulp test",
    "help": "echo --List all available script commands-- && npm-package-user-scripts-list"
  }
}
